<div class="container">
    {{#each beersFromApi}}
        <div class="card">
            <picture><img src="{{this.image_url}}" alt="{{this.name}}"></picture>
            <p>{{this.name}}</p>
            <p>{{this.tagline}}</p>
            <p>{{this.description}}</p>
        </div>
    {{/each}}

</div>